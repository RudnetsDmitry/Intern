<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="QT for DAT rules"
	Version="8,00"
	>
	<Rules>
		<CustomBuildRule
			Name="ui Dat"
			DisplayName="qt ui compile Dat"
			CommandLine="$(UicDIR)\uicx.exe $(InputPath) GeneratedFiles\ui_$(InputName).h [parentIncludePath] [parentClassName]"
			Outputs="GeneratedFiles\ui_$(InputName).h"
			FileExtensions="*.ui"
			ExecutionDescription="$(UicDIR)\uicx.exe $(InputPath) GeneratedFiles\ui_$(InputName).h [parentIncludePath] [parentClassName]"
			>
			<Properties>
				<StringProperty
					Name="parentIncludePath"
					DisplayName="путь для заголовочного файла родительского ui"
					Description="путь для заголовочного файла родительского ui"
					Switch="[value]"
					/>
				<StringProperty
					Name="parentClassName"
					DisplayName="имя класса родительского ui"
					Description="имя класса родительского ui"
					Switch="[value]"
					/>
			</Properties>
		</CustomBuildRule>
		<CustomBuildRule
			Name="moc Dat"
			DisplayName="qt moc compile Dat"
			CommandLine="$(MocDIR)\moc.exe $(InputPath)  [stdafx] -f&quot;$(InputPath)&quot; -o [tmppath]tmp\moc_$(InputName).cpp"
			Outputs="GeneratedFiles\moc_$(InputName).cpp"
			FileExtensions="*.dummy_moc"
			ExecutionDescription="$(MocDIR)\moc.exe $(InputPath)  [stdafx] -f&quot;$(InputPath)&quot; -o [tmppath]tmp\moc_$(InputName).cpp"
			>
			<Properties>
				<BooleanProperty
					Name="stdafx"
					DisplayName="stdafx.h"
					Description="использовать stdafx.h ?"
					Switch="-f&quot;stdafx.h&quot;"
					DefaultValue="true"
				/>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
